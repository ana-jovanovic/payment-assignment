@model IList<PaymentAssignement.ViewModels.VendorPaidAmountViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model != null && Model.Any())
{
    <table class="table">
        <tr>
            <th>
                Vendor Name
            </th>
            <th>
                Total Paid Amount
            </th>
        </tr>

        @foreach (var info in Model)
        {
            <tr>
                <td>
                    @info.VendorName
                </td>
                <td>
                    @info.TotalSum
                </td>
            </tr>
        }
    </table>
}
